{
  "manifest_version": 1,
  "name": "CARM",
  "version": "1",
  "description": "Communicate Among Random Masses",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"],
      "run_at": "document_end"
    }
  ]
}